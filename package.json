{
  "name": "sfz-web-player",
  "version": "0.0.36",
  "description": "SFZ player using the Web Audio API",
  "main": "dist/sfz.js",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "analyze": "webpack --config webpack.prod.js --profile --json > stats.json",
    "build": "webpack --config webpack.dev.js && webpack --config webpack.prod.js",
    "check": "npm run format && npm run lint && npm run build && npm run test:unit",
    "dev": "webpack serve --config webpack.dev.js",
    "format": "prettier --write 'src/**/*.{ts,tsx}' && prettier --write 'tests/**/*.{ts,tsx}'",
    "lint": "tslint --project tsconfig.json && tslint 'tests/**/*.{ts,tsx}'",
    "start": "serve ./dist",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "vitest run",
    "test:integration": "playwright test",
    "test:unit:watch": "vitest",
    "test:integration:ui": "playwright test --ui",
    "test:update": "vitest run --reporter=verbose --update-snapshots",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "sfz",
    "web",
    "player",
    "sampler"
  ],
  "author": "kmturley",
  "license": "CC0-1.0",
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@types/node-fetch": "^2.6.4",
    "@vitejs/plugin-react": "^4.2.0",
    "buffer": "^6.0.3",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "file-loader": "^6.2.0",
    "glob": "^10.3.3",
    "identity-obj-proxy": "^3.0.0",
    "jsdom": "^23.0.0",
    "mini-css-extract-plugin": "^2.7.5",
    "playwright": "^1.40.0",
    "prettier": "^2.8.4",
    "sass": "^1.58.3",
    "sass-loader": "^13.2.0",
    "serve": "^14.2.4",
    "stream": "^0.0.2",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.9.5",
    "vite": "^5.0.0",
    "vitest": "^1.0.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^5.2.2",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@sfz-tools/core": "^0.0.28",
    "browser-fs-access": "^0.33.0",
    "node-fetch": "^2.6.12",
    "xml-js": "^1.6.11"
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  }
}
