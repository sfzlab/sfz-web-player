{
  "name": "sfz-web-player",
  "version": "0.0.36",
  "description": "SFZ player using the Web Audio API",
  "main": "dist/sfz.js",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "analyze": "webpack --config webpack.prod.js --profile --json > stats.json",
    "build": "webpack --config webpack.dev.js && webpack --config webpack.prod.js",
    "check": "npm run format && npm run lint && npm run build && npm run test:unit",
    "dev": "webpack serve --config webpack.dev.js",
    "format": "prettier --write 'src/**/*.{ts,tsx}' && prettier --write 'tests/**/*.{ts,tsx}'",
    "lint": "tslint --project tsconfig.json && tslint 'tests/**/*.{ts,tsx}'",
    "start": "serve ./dist",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "vitest run",
    "test:integration": "playwright test",
    "test:unit:watch": "vitest",
    "test:integration:ui": "playwright test --ui",
    "test:update": "vitest run --reporter=verbose --update-snapshots",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "sfz",
    "web",
    "player",
    "sampler"
  ],
  "author": "kmturley",
  "license": "CC0-1.0",
  "devDependencies": {
    "@playwright/test": "^1.58.0",
    "@types/node-fetch": "^2.6.13",
    "@vitejs/plugin-react": "^5.1.2",
    "buffer": "^6.0.3",
    "copy-webpack-plugin": "^13.0.1",
    "css-loader": "^7.1.3",
    "file-loader": "^6.2.0",
    "glob": "^13.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jsdom": "^27.4.0",
    "mini-css-extract-plugin": "^2.10.0",
    "playwright": "^1.58.0",
    "prettier": "^3.8.1",
    "sass": "^1.97.3",
    "sass-loader": "^16.0.6",
    "serve": "^14.2.5",
    "stream": "^0.0.3",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18",
    "webpack": "^5.104.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.3",
    "webpack-merge": "^6.0.1"
  },
  "dependencies": {
    "@sfz-tools/core": "^0.0.28",
    "browser-fs-access": "^0.38.0",
    "node-fetch": "^3.3.2",
    "xml-js": "^1.6.11"
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  }
}
